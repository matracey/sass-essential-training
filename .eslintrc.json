{
    "env": {
        "browser": true
    },
    "globals": {
        "require": false,
        "process": false
    },
    "rules": {
        "no-bitwise": "error",
        "camelcase": "off",
        "curly": "error",
        "eqeqeq": "error",
        "guard-for-in": "error",
        "no-extend-native": "error",
        "wrap-iife": "off",
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-use-before-define": "off",
        "new-cap": "off",
        "no-caller": "error",
        "no-empty": "error",
        "no-irregular-whitespace": "error",
        "no-new": "off",
        "no-plusplus": "off",
        "quotes": "off",
        "no-undef": "error",
        "no-unused-vars": "error",
        "strict": ["error", "global"],
        "max-params": "off",
        "max-depth": "off",
        "max-statements": "off",
        "complexity": "off",
        "max-len": "off",
        "semi": "off",
        "no-cond-assign": "off",
        "no-debugger": "off",
        "no-eq-null": "off",
        "no-eval": "off",
        "no-unused-expressions": "off",
        "block-scoped-var": "off",
        "no-iterator": "off",
        "linebreak-style": "off",
        "comma-style": ["error"],
        "no-loop-func": "off",
        "no-multi-str": "off",
        "require-yield": "off",
        "valid-typeof": "off",
        "no-proto": "off",
        "no-script-url": "off",
        "no-shadow": "error",
        "dot-notation": "error",
        "no-new-func": "off",
        "no-new-wrappers": "off",
        "no-invalid-this": "off"
    }
}
